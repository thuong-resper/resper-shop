\r\n    return () => {\r\n      dispatch(clearState());\r\n    }; // eslint-disable-next-line\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (isError) {\r\n      enqueueSnackbar(message, { variant: 'error' });\r\n      dispatch(clearState());\r\n    }\r\n\r\n    if (isSuccess) {\r\n      enqueueSnackbar(message, { variant: 'info' });\r\n      history.push('/user/forget-password');\r\n      dispatch(clearState());\r\n    } // eslint-disable-next-line\r\n  }, [isError, isSuccess]);\r\n\r\n  // form\r\n  const {\r\n    register,\r\n    setValue,\r\n    handleSubmit,\r\n    control,\r\n    formState: { errors },\r\n  } = useForm({\r\n    defaultValues: {\r\n      email: data.value,\r\n    },\r\n    mode: 'onBlur',\r\n    resolver: yupResolver(schema),\r\n  });\r\n\r\n  const { isSubmitting } = useFormState({ control });\r\n\r\n  // submit form\r\n  const onSubmit = async (value) => {\r\n    if (value) {\r\n      setValues({ value: value.email });\r\n      actionPostForgotPassword(value);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <Grid container className={classes.root}>\r\n      <Grid item xs={false} sm={4} md={7} className={classes.image} />\r\n      <Grid\r\n        item\r\n        xs={12}\r\n        sm={8}\r\n        md={5}\r\n        component={Paper}\r\n        elevation={6}\r\n        square\r\n        className={classes.pageCenter}\r\n      >\r\n        <Box className={classes.paper}>\r\n          <Typography variant=\"h5\">\r\n            <b>Đặt lại mật khẩu</b>\r\n          </Typography>\r\n          <Typography variant=\"body1\" style={{ marginTop: '1rem' }}>\r\n            Nhập email được liên kết với tài khoản và chúng tôi sẽ gửi email kèm theo hướng dẫn để\r\n            đặt lại mật khẩu.\r\n          </Typography>\r\n          <Form onSubmit={handleSubmit(onSubmit)}>\r\n            <Input\r\n              {...register('email')}\r\n              id=\"email\"\r\n              type=\"text\"\r\n              placeholder=\"Email\"\r\n              name=\"email\"\r\n              error={!!errors.email}\r\n              helperText={errors?.email?.message}\r\n              disabled={isSubmitting}\r\n              InputProps={{\r\n                endAdornment: (\r\n                  <InputAdornment position=\"end\">\r\n                    <MailOutlineIcon color=\"disabled\" />\r\n                  </InputAdornment>\r\n                ),\r\n              }}\r\n            />\r\n\r\n            <Box position=\"relative\">\r\n              <SignupBtn disabled={isSubmitting}>Gửi hướng dẫn</SignupBtn>\r\n              {isSubmitting && <CircularProgress size={24} className={classes.buttonProgress} />}\r\n            </Box>\r\n          </Form>\r\n          {/* sign up */}\r\n          <Box display=\"flex\" justifyContent=\"center\">\r\n            <Typography variant=\"body2\">Chưa phải thành viên?&nbsp;</Typography>\r\n            <Link to=\"/register\">\r\n              <Typography variant=\"body2\" color=\"secondary\">\r\n                <b>Đăng ký</b>\r\n              </Typography>\r\n            </Link>\r\n          </Box>\r\n        </Box>\r\n      </Grid>\r\n    </Grid>\r\n  );\r\n};\r\n\r\nexport default ForgetForm;\r\n","/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/components/Notification/Notification.js",[],["818","819"],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/NotistackWrapper.js",[],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/pages/SignupPage/ActiveEmail.js",[],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/pages/SignupPage/CheckEmailActive.js",[],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/pages/LoginPage/CheckEmailForget.js",[],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/pages/LoginPage/CreateAPassword.js",[],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/components/Header/User/MenuUser.js",["820","821","822","823","824","825","826"],"/mnt/c/Users/kangj/Desktop/ecommerce/frontend/src/components/Header/User/styles.js",["827","828"],"/mnt/c/Users/kangj/Desktop/ecommerce/fro